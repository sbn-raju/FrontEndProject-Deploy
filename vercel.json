{
  "version": 2,
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/controllers/(.*)",
      "dest": "/src/controllers/$1"
    },
    {
      "src": "/routes/(.*)",
      "dest": "/src/routes/$1"
    },
    {
      "src": "/views/(.*)",
      "dest": "/views/$1"
    },
    {
      "src": "/",
      "dest": "/src/index.js"
    }
  ],
  "builds": [
    {
      "src": "/public/(.*)",
      "use": "@vercel/static"
    },
    {
        "src":"src/*.js",
        "use":"@vercel/node"
    }
        
  ]
}
